{
  "name": "nextjs-app-v1",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run dx:next",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "lint:strict": "next lint '*/**/*.{js,ts,tsx}'",
    "prettier": "prettier --write */**/*.{js,json,ts,tsx,scss,css,md}",
    "migrate-dev": "prisma migrate dev",
    "migrate": "prisma migrate deploy",
    "generate": "prisma generate",
    "prisma-studio": "prisma studio",
    "db-seed": "prisma db seed",
    "db-reset": "prisma migrate dev reset",
    "dx:next": "run-s migrate-dev && next dev",
    "dx:prisma-studio": "npm run prisma-studio",
    "dx": "run-p dx:* --print-label",
    "prebuild": "run-s generate migrate",
    "postinstall": "npm run generate",
    "prepare": "husky install",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test-e2e": "playwright test"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@emotion/cache": "^11.11.0",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
